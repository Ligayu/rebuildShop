<view class="content"><view class="adSwiper"><swiper class="adsImg" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="true"><block wx:for="{{adImg}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item.image_url}}" mode="aspectFill"></image></swiper-item></block></swiper></view><view class="navs"><block wx:for="{{navs}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="navsContent"><navigator url="{{item.url}}"><text class="{{[item.icon]}}"></text><text class="text">{{item.text}}</text></navigator></view></block></view><block wx:for="{{goods}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="category"><view class="cover" style="{{('background:url('+item.banner+');background-size: 100% auto;')}}"><text>{{item.name}}</text></view><view class="goods"><block wx:for="{{item.goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="goodsCover"><image class="goodsImg" src="{{item.list_pic_url}}" mode="aspectFit" data-event-opts="{{[['tap',[['goodsDetail',['$0'],[[['goods','',index],['goodsList','',index,'id']]]]]]]}}" bindtap="__e"></image><view class="goodsTitle">{{''+item.name+''}}</view><view class="goodsPrice">{{"￥"+item.min_retail_price}}</view></view></block></view><view data-event-opts="{{[['tap',[['clickLink',[index]]]]]}}" class="more" bindtap="__e">{{'点击查看更多'+item.name+''}}</view></view></block></view>