
.adsImg{
	height: 400rpx;
}
.adsImg image{
		width: 100%;
		height:100% ;
}
.navs {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      margin: 50rpx auto;
}
.navs .navsContent{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.navs .navsContent .iconfont{
	display: block;
	font-size: 60rpx;
	text-align: center;
}
.navs .navsContent .text{
	margin-top: 20rpx;
	display: block;
	text-align: center;
	font-size: 20rpx;
}
.category{
	margin:30rpx 20rpx
}
.category .more {
	margin: 18rpx auto 0;
	width: 100%;
	font-size: 16rpx;
	text-align: center;
}
.category .cover{
	/* background: url(../../static/navs/01.jpg) no-repeat; */
	text-align: center;
	line-height: 160rpx;
	height: 160rpx;
	color: rgb(255,254,250);
	border-radius: 10rpx;
}
.category .goods {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 20rpx;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.category .goods .goodsCover{
	width: 200rpx;
	margin: 10rpx 16rpx;
}
.category .goods .goodsCover .goodsImg{
	width: 100%;
	height: 200rpx;
	background-color: #f9f9f9;
}
.category .goods .goodsCover .goodsTitle{
	text-align: center;
	font-size: 20rpx;
}
.category .goods .goodsCover .goodsPrice{
	text-align: center;
	color:#ff5370 ;
	font-size: 18rpx;
}

